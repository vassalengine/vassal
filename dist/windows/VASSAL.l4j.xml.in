<launch4jConfig>
  <dontWrapJar>true</dontWrapJar>
  <headerType>gui</headerType>
  <outfile>VASSAL.exe</outfile>
  <errTitle>VASSAL</errTitle>
  <cmdLine></cmdLine>
  <chdir>.</chdir>
  <priority>normal</priority>
  <supportUrl>http://www.vassalengine.org/forum</supportUrl>
  <stayAlive>false</stayAlive>
  <icon>VASSAL.ico</icon>
  <classPath>
    <mainClass>VASSAL.launch.ModuleManager</mainClass>
    <cp>lib\Vengine.jar</cp>
  </classPath>
  <jre>
    <path>jre</path>
    %JREOPTS%
  </jre>
  <versionInfo>
    <fileVersion>%NUMVERSION%.0</fileVersion>
    <txtFileVersion>%FULLVERSION%</txtFileVersion>
    <fileDescription>VASSAL</fileDescription>
    <copyright>The VASSAL Team</copyright>
    <productVersion>%NUMVERSION%.0</productVersion>
    <txtProductVersion>%FULLVERSION%</txtProductVersion>
    <productName>VASSAL</productName>
    <internalName>VASSAL</internalName>
    <originalFilename>VASSAL.exe</originalFilename>
  </versionInfo>
</launch4jConfig>
